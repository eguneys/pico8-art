pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
dbg = ""

points = {
	0, 0, 20, 0,
	20, 20, 0, 20, 0, 0
}

function draw_points(points)
	for i=1,#points-2,2 do
  	line(points[i], points[i+1],
  		points[i+2],	points[i+3])
 end
end

function init_sin_ps(phase, amplitude)
	local res = {}
	for i=0,16 do
	 add(res, i * 8)
		add(res, 64 + sin((i/16 + phase) % 1)*amplitude)
	end
	return res
end

function init_axis_x()
	return {
		0, 64, 128, 64
	}
end

function init_axis_y()
	return {
		64, 0, 64, 128
	}
end

axis_x = init_axis_x()
axis_y = init_axis_y()

-- sp = init_sin_ps(0, 8)
sp = init_sin_ps(0.5, 64)



function _init()
	phase = 0
	amplitude = 0
end

function _update()

 dbg = phase

	phase += 0.05
	
	amplitude += 1
	amplitude %= 48

	sp = init_sin_ps(phase, amplitude)
end

function _draw()
	cls()
	
	-- draw_points(points)
	
	--draw_points(axis_x)
	--draw_points(axis_y)
	draw_points(sp)
	
	--print(dbg, 0, 120)
end
